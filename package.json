{
  "name": "graphtransliterator",
  "type": "commonjs",
  "version": "0.5.1",
  "description": "A graph-based transliteration tool",
  "homepage": "https://github.com/seanpue/graphtransliterator-node",
  "author": "A. Sean Pue <pue@msu.edu> (https://seanpue.com)",
  "files": [
    "dist",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/main.js",
  "keywords": [
    "transliteration",
    "language",
    "graph"
  ],
  "devDependencies": {
    "ajv": "^6.12.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^10.2.0",
    "minami": "^1.2.3",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "test": "jest --coverage",
    "build": "webpack",
    "update": "node ./scripts/updateTransliterators.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpue/graphtransliterator-node.git"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^24.9.0",
    "core-js": "^3.6.5"
  },
  "bugs": {
    "url": "https://github.com/seanpue/graphtransliterator-node/issues"
  },
  "directories": {
    "lib": "lib"
  }
}
