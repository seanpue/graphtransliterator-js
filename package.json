{
  "name": "graphtransliterator",
  "version": "0.3.0",
  "description": "A graph-based transliteration tool",
  "homepage": "https://github.com/seanpue/graphtransliterator-node",
  "author": {
    "name": "A. Sean Pue",
    "email": "pue@msu.edu",
    "url": "https://seanpue.com"
  },
  "files": [
    "dist"
  ],
  "main": "lib/index.js",
  "keywords": [
    "transliteration",
    "language",
    "graph"
  ],
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "npm": ">= 12.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "test": "jest",
    "build": "webpack --config webpack.config.js",
    "update": "python scripts/update_transliterators.py"
  },
  "repository": "https://github.com/seanpue/graphtransliterator-node.git",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-jest": "^24.9.0",
    "core-js": "^3.4.8"
  }
}
