{
  "name": "graphtransliterator",
  "version": "0.2.0",
  "description": "A graph-based transliteration tool",
  "homepage": "https://github.com/seanpue/graphtransliterator-node",
  "author": {
    "name": "A. Sean Pue",
    "email": "pue@msu.edu",
    "url": "https://seanpue.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "transliteration",
    "language",
    "graph"
  ],
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1"
  },
  "engines": {
    "npm": ">= 12.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "test": "jest"
  },
  "repository": "https://github.com/seanpue/graphtransliterator-node.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
